{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.3",
        "bref/bref": "^2.1.4",
        "bref/symfony-bridge": "^0.2.2",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "knplabs/knp-markdown-bundle": "^1.9",
        "knplabs/knp-paginator-bundle": "^5.5",
        "knpuniversity/oauth2-client-bundle": "^2.8",
        "league/oauth2-facebook": "^2.0",
        "liip/imagine-bundle": "^2.6",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "sentry/sentry-symfony": "^4.11.0",
        "stof/doctrine-extensions-bundle": "^1.6",
        "symfony/asset": "^6.3.0",
        "symfony/console": "^6.3.0",
        "symfony/dotenv": "^6.3.0",
        "symfony/expression-language": "^6.3.0",
        "symfony/flex": "^1.13",
        "symfony/form": "^6.3.0",
        "symfony/framework-bundle": "^6.3.0",
        "symfony/http-client": "^6.3.0",
        "symfony/intl": "^6.3.0",
        "symfony/mailer": "^6.3.0",
        "symfony/mime": "^6.3.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/notifier": "^6.3.0",
        "symfony/process": "^6.3.0",
        "symfony/property-access": "^6.3.0",
        "symfony/property-info": "^6.3.0",
        "symfony/runtime": "^6.3.0",
        "symfony/security-bundle": "^6.3.0",
        "symfony/serializer": "^6.3.0",
        "symfony/string": "^6.3.0",
        "symfony/translation": "^6.3.0",
        "symfony/twig-bundle": "^6.3.0",
        "symfony/validator": "^6.3.0",
        "symfony/web-link": "^6.3.0",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/yaml": "^6.3.0",
        "symfonycasts/verify-email-bundle": "^1.5",
        "twig/extra-bundle": "^3.3",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.3",
        "twig/twig": "^3.3",
        "vich/uploader-bundle": "^1.18"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.8",
        "symfony/browser-kit": "^6.3.0",
        "symfony/css-selector": "^6.3.0",
        "symfony/debug-bundle": "^6.3.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/phpunit-bridge": "^6.3.0",
        "symfony/stopwatch": "^6.3.0",
        "symfony/twig-bundle": "^6.3.0",
        "symfony/var-dumper": "^6.3.0",
        "symfony/web-profiler-bundle": "^6.3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "env": "cd tools && castor app:tools:env"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.3.0"
        }
    }
}
