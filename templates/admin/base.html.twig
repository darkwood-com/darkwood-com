        {% for flashMessage in app.session.flashbag.get('error') %}<div class="alert alert-danger" style="margin-top: 40px;margin-right: 8px;"><i class="fa fa-bomb"></i> {{ flashMessage }}</div>{% endfor %}


        {% block content %}
        {% endblock content %}
    </div>
</div>
