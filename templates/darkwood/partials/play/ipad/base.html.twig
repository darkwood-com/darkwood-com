{% extends "common/base.html.twig" %}

{% block layout %}layout layout-darkwood{% endblock %}

{% block body %}
    <div class="container-fluid theme theme-darkwood">
        <div class="row theme-body">
            <div class="col-md-12">
                <div class="row theme-content{% if showLinks is defined %} show_links{% endif %}">
                    <div class="col-md-12 theme-content-top">
                        {% block content %}{% endblock %}
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
